{
  "name": "lib-upng",
  "version": "2.0.1",
  "description": "UPNG.js",
  "keywords": [
    "UPNG",
    "UPNG.js",
    "cjs",
    "umd",
    "esm"
  ],
  "homepage": "https://github.com/fisker/lib-upng#readme",
  "bugs": {
    "url": "https://github.com/fisker/lib-upng/issues"
  },
  "repository": "fisker/lib-upng",
  "funding": "https://github.com/fisker/lib-upng?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.common.js",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/{node_modules,UPNG.js}/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "nyc yarn test",
    "update-submodule": "git submodule update --remote --merge"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "pako": "2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@fisker/commitlint-config": "2.0.4",
    "@fisker/eslint-config": "10.1.4",
    "@fisker/eslint-config-ava": "2.0.5",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/prettier-config": "5.0.6",
    "ava": "4.2.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "4.0.1",
    "eslint": "8.15.0",
    "husky": "8.0.1",
    "is-png": "3.0.1",
    "lint-staged": "12.4.1",
    "markdownlint-cli": "0.31.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "rollup": "2.77.2",
    "sort-package-json": "1.57.0",
    "stylelint": "14.8.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
